<%- include("./partials/head", {pageTitle: "Registrarme", stylesheet: "register.css"}) %>



    <header>
        <p class="barrasMenu"><i class="fas fa-bars"></i></p>
        <a href="/">
            <img class="logo" src="images/logoConTitulo.png" height="45px">
        </a>
    </header>
    
    <div class="contenedor-logo">
        <a href="/">
            <img class="logo2" src="/images/isologoNaranja.png">
        </a>
    </div>
    
    <form action="/" method="">

        <div class="titulo">
            <h2>COMPLETA TUS DATOS</h2>
        </div>

        <div class="campo-form">
            <label>Nombre y apellido</label>
            <input type="text" name="nombreYApellido" required>
        </div>

        <div class="campo-form">
            <label>Nombre de usuario</label>
            <input type="text" name="nombreUsuario" required>
        </div>
        <div class="campo-form">
            <label>Email</label>
            <input type="email" name="emailUsuario" required>
        </div>
        <div class="campo-form">
            <label>Contraseña</label>
            <input type="password" name="contraseniaUsuario" required>
        </div>
        <div class="campo-form">
            <label>Confirmar contraseña</label>
            <input type="password" name="contraseniaUsuario" required>
        </div>

        <div class="caja-boton">
            <button type="submit">CREA TU CUENTA</button>
        </div>
    </form>


</body>

</html>